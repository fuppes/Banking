<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Banking.Letzte_Überweisungen"
             Title="Letzte_Überweisungen">
    <VerticalStackLayout>
        <Label 
            Text="Letzte Überweisungen:"
            Style="{StaticResource Headline}"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Label
            Text="       uhuhuhhuhhuh  "
            TextColor="white"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />


        <Label 
            Text=" Keine älteren Überweisungen, aktualisieren sie die Seite."
            TextColor="Red"
            Style="{StaticResource SubHeadline}"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Label
            Text="       uhuhuhhuhhuh  "
            TextColor="white"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />


        <Button
            x:Name="CounterBtn15"
            BackgroundColor="Black"
            Text="Zurück zu Überweisungen" 
            SemanticProperties.Hint="go to Überweisung-page"
            Clicked="CounterBtn15_Clicked"
            HorizontalOptions="Center" />



    </VerticalStackLayout>
</ContentPage>